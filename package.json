{
    "name": "blockly-field-date",
    "version": "1.0.0",
    "description": "A Blockly advanced date picker field that uses the Google Closure date picker",
    "scripts": {
        "audit:fix": "blockly-scripts auditFix",
        "build": "gulp build",
        "clean": "blockly-scripts clean",
        "lint": "blockly-scripts lint",
        "predeploy": "npm run build && blockly-scripts predeploy",
        "prepublishOnly": "npm run clean && npm run build",
        "start": "npm run build && blockly-scripts start",
        "test": "npm run build && blockly-scripts test"
    },
    "main": "dist/date_compressed.js",
    "author": "Blockly Team",
    "keywords": [
        "blockly",
        "field",
        "date",
        "datepicker"
    ],
    "homepage": "https://github.com/tomas-berg/blockly-field-date.git#readme",
    "bugs": {
        "url": "https://github.com/tomas-berg/blockly-field-date/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/tomas-berg/blockly-field-date.git"
    },
    "license": "Apache-2.0",
    "directories": {
        "dist": "dist",
        "src": "src"
    },
    "files": [
        "dist",
        "src"
    ],
    "devDependencies": {
        "@blockly/dev-scripts": "^1.2.14",
        "@blockly/dev-tools": "^3.0.7",
        "blockly": "^7.20211209.0",
        "eslint": "^8.11.0",
        "google-closure-compiler": "^20200920.0.0",
        "google-closure-library": "^20200830.0.0",
        "gulp": "^4.0.2",
        "gulp-sourcemaps": "^2.6.5"
    },
    "peerDependencies": {
        "blockly": "^7.20211209.4"
    },
    "engines": {
        "node": ">=8.0.0"
    },
    "browserslist": [
        "defaults",
        "IE 11",
        "IE_Mob 11"
    ],
    "dependencies": {
        "luxon": "^2.3.1"
    }
}
